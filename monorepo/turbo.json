{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": ["package.json"],
    "tasks": {
      "build": {
        "outputs": ["dist/**"],
        "dependsOn": ["^build"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "start": {
        "cache": false
      }
    }
  }
  